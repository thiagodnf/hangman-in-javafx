<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<DialogPane maxHeight="-Infinity" prefWidth="330.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="thiagodnf.hangmanfx.controller.SettingsController">
   <content>
      <VBox prefHeight="25.0" prefWidth="330.0" spacing="14.0">
         <children>
            <Label text="Dictionary" />
            <ComboBox fx:id="dictionaryComboBox" />
            <Label text="Categories" />
            <CheckBox id="fruits" fx:id="fruitsCheckBox" mnemonicParsing="false" text="Fruits" />
            <CheckBox id="carMakers" fx:id="carMakersCheckBox" mnemonicParsing="false" text="Car Makers" />
         </children>
      </VBox>
   </content>
</DialogPane>
